{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/favicon-*.png",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "lua",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/lua/**/*.lua"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": ["*/api/**"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "5m",
        "timeout": "10s",
        "strategy": "performance"
      }
    }
  ]
}
